{
  "Applications": {
    "$1": {
      "Application": {
        "Description": "Alfresco API",
        "Name": "Alfresco API"
      },
      "LookupConfig": {
        "PathRegex": "/alfresco.*"
      },
      "LowCodeUnit": {
        "APIRoot": "https://{{PUBLIC_IP_ADDRESS_1.value}}",
        "Security": "~",
        "Type": "API"
      },
      "Processor": {
        "InboundPath": "alfresco",
        "Type": "Proxy"
      }
    },
    "$2": {
      "Application": {
        "Description": "Alfresco Admin",
        "Name": "Alfresco Admin"
      },
      "LookupConfig": {
        "PathRegex": "/admin/alfresco.*"
      },
      "LowCodeUnit": {
        "SPARoot": "https://{{PUBLIC_IP_ADDRESS_1.value}}",
        "Type": "SPA"
      },
      "Processor": {
        "InboundPath": "admin/alfresco",
        "Type": "Proxy"
      }
    }
  }
}
