{
  "Applications": {
    "{{$1}}-alfresco-api": {
      "Details": {
        "Description": "Alfresco API",
        "Name": "Alfresco API"
      },
      "LookupConfig": {
        "PathRegex": "/alfresco.*"
      },
      "Package": {
        "Details": {
          "APIRoot": "{{PUBLIC_IP_ADDRESS_1}}",
          "Security": "~"
        },
        "Type": "APILowCodeUnit"
      },
      "Processor": {
        "Details": {
          "InboundPath": "/alfresco"
        },
        "Type": "ProxyProcessor"
      }
    },
    "{{$2}}-alfresco-admin": {
      "Details": {
        "Description": "Alfresco Admin",
        "Name": "Alfresco Admin"
      },
      "LookupConfig": {
        "PathRegex": "/.*"
      },
      "Package": {
        "Details": {
          "SPARoot": "{{PUBLIC_IP_ADDRESS_1}}"
        },
        "Type": "SPALowCodeUnit"
      },
      "Processor": {
        "Details": {
          "InboundPath": "/"
        },
        "Type": "ProxyProcessor"
      }
    }
  },
  "Environments": {
    "$#.PrimaryEnvironment": {
      "Clouds": {
        "{{AZURE_CLOUD_LOOKUP}}": {}
      }
    }
  }
}