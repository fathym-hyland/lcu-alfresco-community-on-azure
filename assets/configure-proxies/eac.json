{
  "Applications": {
    "{{PROJECT_LOOKUP}}-alfresco-api": {
      "Details": {
        "Description": "Alfresco API",
        "Name": "Alfresco API"
      },
      "LookupConfig": {
        "PathRegex": "/alfresco.*"
      },
      "Package": {
        "Details": {
          "APIRoot": "{{PUBLIC_IP_ADDRESS_1}}",
          "Security": "~"
        },
        "Type": "APILowCodeUnit"
      },
      "Processor": {
        "Details": {
          "InboundPath": "/alfresco"
        },
        "Type": "ProxyProcessor"
      }
    },
    "{{PROJECT_LOOKUP}}-alfresco-admin": {
      "Details": {
        "Description": "Alfresco Admin",
        "Name": "Alfresco Admin"
      },
      "LookupConfig": {
        "PathRegex": "/.*"
      },
      "Package": {
        "Details": {
          "SPARoot": "{{PUBLIC_IP_ADDRESS_1}}"
        },
        "Type": "SPALowCodeUnit"
      },
      "Processor": {
        "Details": {
          "InboundPath": "/"
        },
        "Type": "ProxyProcessor"
      }
    }
  },
  "Environments": {
    "{{EaC.PrimaryEnvironment}}": {
      "Clouds": {
        "{{AZURE_CLOUD_LOOKUP}}": {}
      }
    }
  },
  "Projects": {
    "{{PROJECT_LOOKUP}}": {
      "Details": {
        "Name": "{{#if (lookup EaC.Projects PROJECT_LOOKUP)}}{{#if Details.Name}}{{Details.Name}}{{/if}}{{else}}Alfresco Community on Azure with Bitnami{{/if}}",
        "Description": "{{#if (lookup EaC.Projects PROJECT_LOOKUP)}}{{#if Details.Description}}{{Details.Description}}{{/if}}{{else}}An Alfresco Community instance deployed to Azure. Integrated with the Alfresco portal and an Angular app starting point for development with Alfresco Application Development Framework (ADF).{{/if}}"
      },
      "ApplicationLookups": [
        "{{PROJECT_LOOKUP}}-alfresco-api",
        "{{PROJECT_LOOKUP}}-alfresco-admin"
      ]
    }
  }
}